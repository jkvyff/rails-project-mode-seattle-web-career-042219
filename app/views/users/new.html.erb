<center>
<h1>Create User</h1>
<%= form_for @user do |f| %>
    <label>Username</label><br>
        <%= f.text_field :username%><br>
    <label>Password</label><br>
        <%= f.password_field :password%><br>
      <%= f.submit %>
<% end %>

<div id='warning'>
    <div><%= flash[:warning] %> </div>
</div>